generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model accounts {
  username   String    @unique(map: "unique_usernames") @db.VarChar(31)
  email      String    @id @db.VarChar(31)
  password   String    @db.VarChar(62)
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
